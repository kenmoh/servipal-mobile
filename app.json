{
  "expo": {
    "name": "ServiPal",
    "slug": "qp_mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-iphone.png",
    "scheme": "servipal",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-dark.png",
      "resizeMode": "cover",
      "backgroundColor": "#18191c"
      
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.servipal"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-icon.png",
        "backgroundColor": "#18191c"
        
      },
      "package": "com.servipal"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
       [
       "@rnmapbox/maps",
        {
          "RNMapboxMapsDownloadToken": "sk.eyJ1Ijoia2VubW9oIiwiYSI6ImNtM2ZsNHRjMDA0MG4ya3NjM3p2ZXFoNnUifQ.N4lJo-GrsP4tvIYWH7cPHw",
          "RNMapboxMapsVersion": "11.0.0"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "53677c06-debc-41e7-9bdc-4280d6622175"
      }
    }
  }
}
