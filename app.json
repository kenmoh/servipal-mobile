{
  "expo": {
    "name": "ServiPal",
    "slug": "servi-pal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon-iphone.png",
    "scheme": "servipal",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-dark.png",
      "resizeMode": "cover"
      
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.swiftmart"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-icon.png"
        
      },
      "package": "com.servipal"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "53677c06-debc-41e7-9bdc-4280d6622175"
      }
    }
  }
}
